			TwinMax: function (){
				var jQuerycircleCursor = jQuery(".cursor");

				function moveCursor(e) {
					var t = e.clientX + "px",
					a = e.clientY + "px";
					TweenMax.to(jQuerycircleCursor, .2, {
						left: t,
						top: a,
						ease: "Power1.easeOut"
					})
				}

				function zoomCursor(e) {
					TweenMax.to(jQuerycircleCursor, .1, {
						scale: 3,
						ease: "Power1.easeOut"
					}), jQuery(jQuerycircleCursor).removeClass("cursor-close")
				}

				function closeCursor(e) {
					TweenMax.to(jQuerycircleCursor, .1, {
						scale: 3,
						ease: "Power1.easeOut"
					}), jQuery(jQuerycircleCursor).addClass("cursor-close")
				}

				function defaultCursor(e) {
					TweenLite.to(jQuerycircleCursor, .1, {
						scale: 1,
						ease: "Power1.easeOut"
					}), jQuery(jQuerycircleCursor).removeClass("cursor-close")
				}
				jQuery(window).on("mousemove", moveCursor),
				jQuery("a, button, .zoom-cursor").on("mouseenter", zoomCursor),
				jQuery(".trigger-close").on("mouseenter", closeCursor),
				jQuery("a, button, .zoom-cursor, .trigger-close, .trigger-plus").on("mouseleave", defaultCursor);
			},
			
			



